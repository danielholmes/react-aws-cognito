{
  "name": "@dhau/react-aws-cognito",
  "version": "0.0.4",
  "type": "module",
  "description": "Context and hooks to manage an AWS Cognito authenticated user in React.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "format": "prettier -w \"src/**/*.ts\"",
    "format:check": "prettier -c \"src/**/*.ts\"",
    "ts": "tsc -p tsconfig.esm.json --noEmit",
    "prebuild": "run-p format:check test:all ts",
    "test": "mocha -w",
    "test:all": "echo 'TODO: mocha'",
    "build:esm": "rollup --config rollup.config.js",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build": "npm-run-all -s clean -p build:*",
    "npm:publish": "npm publish --access public",
    "deploy": "run-s build npm:publish",
    "prepare": "husky install"
  },
  "author": "Daniel Holmes",
  "license": "MIT",
  "dependencies": {
    "@dhau/lang": "^0.0.8",
    "date-fns": "^2.29.3"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/chai": "^4.3.4",
    "@types/lodash-es": "^4.17.6",
    "@types/mocha": "^10.0.1",
    "@types/react": ">=16.0.0",
    "amazon-cognito-identity-js": "^6.0.0",
    "chai": "^4.3.7",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "mocha": "^10.2.0",
    "msw": "^0.49.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0",
    "rollup": "^3.10.0",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "amazon-cognito-identity-js": ">=6.0.0",
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  }
}
